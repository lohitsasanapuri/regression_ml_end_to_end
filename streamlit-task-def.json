{
  "family": "housing-streamlit",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::739623013917:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::739623013917:role/ecs_s3_access",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "housing-streamlit",
      "image": "739623013917.dkr.ecr.us-east-1.amazonaws.com/housing-streamlit:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8501,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "STREAMLIT_SERVER_PORT",
          "value": "8501"
        },
        {
          "name": "STREAMLIT_SERVER_ADDRESS",
          "value": "0.0.0.0"
        },
        {
          "name": "STREAMLIT_SERVER_BASEURLPATH",
          "value": "/dashboard"
        },
        {
          "name": "STREAMLIT_BROWSER_GATHERUSAGESTATS",
          "value": "false"
        },
        {
          "name": "API_URL",
          "value": "http://housing-api-alb-945997111.us-east-1.elb.amazonaws.com/predict"
        }
      ]
    }
  ]
}
